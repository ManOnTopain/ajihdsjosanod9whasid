local InfJump = COB("World", {
["Name"] = "InfJump",
["Function"] = function(Call)
if Call then
game:GetService("UserInputService").JumpRequest:connect(function()
game:GetService("Players").LocalPlayer.Character:FindFirstChildOfClass("Humanoid"):ChangeState("Jumping")
end)
end
end,
["Default"] = false,
["HoverText"] = "Infinite Jump Yas"
})

local MikeESP = COB("World", {
["Name"] = "MikeESP",
["Function"] = function(Call)
if Call then
for i, v in pairs(game.Players:GetChildren()) do
if v.UserId == lplr.UserId then

else
local ESPLOCATE = v.Character
local ESP = Instance.new("Highlight", ESPLOCATE)
ESP.DepthMode = Enum.HighlightDepthMod.AlwaysOnTop
ESP.Enabled = true
ESP.Name = "Highlight"
ESP.FillColor = Color3.new(0,0,0)
ESP.FillTransparency = 1
ESP.OutlineColor = Color3.new(51,51,51)
ESP.OutlineTransparency = 0
end
end

else
for i, v in pairs(players:GetChildren()) do
if v.Character:FindFirstChild("Highlight") then
v.Character.Highlight:Destroy()
end
end
end
end,
})

local kills = COB("Utility", {
["Name"] = "Kills",
["Function"] = function (Call)
if Call then
leaderstats.Kills.Value += 100
end
end,
["Default"] = false,
["HoverText"] = "+ 100 kills"
})






