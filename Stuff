local FinallyLegit = COB("World", {
["Name"] = "Frickin gravity fly",
["Function"] = function(Call)
if Call then
game.Workspace.Gravity = 0
else
game.Workspace.Gravity = 190.21
end
end,
["Default"] = false,
["HoverText"] = "frickin understand it bro"
})

