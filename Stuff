createwarning("Btw", "Im making this script in tablet", 5)

local UserTroll = COB("Utility", {
["Name"] = "Stupid Root Disabler",
["Function"] = function(Call)
if Call then
if character:FindFirstChild("HumanoidRootPart") then
local clone = humanoidroot:Clone()
clone.Parent = workspace
humanoidroot:Destroy()
end
else
if not character:FindFirstChild("HumanoidRootPart") then
clone.Parent = character
end
end
end,
["Default"] = false,
["HoverText"] = "atleast u can fly to other base then disable this"
})

local FastSpeed = COB("World", {
["Name"] = "FastSpeed",
["Function"] = function(Call)
if Call then
humanoid.WalkSpeed = 60
else
humanoid.WalkSpeed = 23
end
end,
["Default"] = false,
["HoverText"] = "60 to 23"
})

local FastFly = COB("World", {
["Name"] = "FastFly",
["Function"] = function(Call)
if Call then
workspace.Gravity = 0
humanoid.WalkSpeed = 60
while Call do
game:GetService("Players").LocalPlayer.Character:FindFirstChildOfClass("Humanoid"):ChangeState("Jumping")
wait(0.2)
game:GetService("Players").LocalPlayer.Character:FindFirstChildOfClass("Humanoid"):ChangeState("Falling")
wait(0.2)
end
else
workspace.Gravity = 180
humanoid.WalkSpeed = 23
end
end,
["Default"] = false,
["HoverText"] = "60 to 23"
})

